{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "decorators": true,
            "dynamicImport": true
        },
        "transform": {
            "react": {
                "runtime": "automatic",
                "development": false,
                "refresh": false
            },
            "optimizer": {
                "globals": {
                    "vars": {
                        "__DEBUG__": "false"
                    }
                }
            }
        },
        "target": "es2022",
        "loose": false,
        "externalHelpers": false,
        "keepClassNames": true,
        "minify": {
            "compress": {
                "unused": true,
                "dead_code": true
            },
            "mangle": true
        }
    },
    "module": {
        "type": "es6",
        "strict": true,
        "strictMode": true,
        "lazy": false,
        "noInterop": false
    },
    "minify": true,
    "isModule": true
}